{
  "order": 0,
  "template": "logs*",
  "settings": {
    "index.refresh_interval": "5s",
    "index.number_of_shards": "6"
  },
  "mappings": {
    "match": {
      "numeric_detection": true,
      "dynamic_templates": [
        {
          "string_fields": {
            "mapping": {
              "index": "not_analyzed",
              "omit_norms": true,
              "type": "string"
            },
            "match_mapping_type": "string",
            "match": "*"
          }
        }
      ],
      "date_detection": true,
      "properties": {
        "reason": {
          "properties": {
            "fqdn": {
              "index": "not_analyzed",
              "type": "string"
            },
            "ip": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "request": {
          "properties": {
            "ip": {
              "index": "not_analyzed",
              "type": "string"
            },
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "geoip": {
          "properties": {
            "timezone": {
              "index": "not_analyzed",
              "type": "string"
            },
            "area_code": {
              "type": "long"
            },
            "ip": {
              "index": "not_analyzed",
              "type": "string"
            },
            "latitude": {
              "type": "double"
            },
            "continent_code": {
              "index": "not_analyzed",
              "type": "string"
            },
            "city_name": {
              "index": "not_analyzed",
              "type": "string"
            },
            "country_code2": {
              "index": "not_analyzed",
              "type": "string"
            },
            "country_name": {
              "index": "not_analyzed",
              "type": "string"
            },
            "dma_code": {
              "type": "long"
            },
            "country_code3": {
              "index": "not_analyzed",
              "type": "string"
            },
            "location": {
              "type": "double"
            },
            "region_name": {
              "index": "not_analyzed",
              "type": "string"
            },
            "real_region_name": {
              "index": "not_analyzed",
              "type": "string"
            },
            "postal_code": {
              "index": "not_analyzed",
              "type": "string"
            },
            "longitude": {
              "type": "double"
            }
          }
        },
        "logged": {
          "format": "yyyy-MM-dd'T'HH:mm:ssZ",
          "type": "date"
        },
        "virus_total_request": {
          "properties": {
            "processed": {
              "format": "yyyy-MM-dd'T'HH:mm:ssZ",
              "type": "date"
            },
            "report_received": {
              "format": "yyyy-MM-dd'T'HH:mm:ssZ",
              "type": "date"
            },
            "status": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "accuracy": {
          "index": "not_analyzed",
          "type": "object"
        },
        "action": {
          "index": "not_analyzed",
          "type": "string"
        },
        "client": {
          "index": "not_analyzed",
          "type": "string"
        }
      }
    },
    "aliases": {

    }
  },
  "aliases": {
    "logs": {

    }
  }
}
